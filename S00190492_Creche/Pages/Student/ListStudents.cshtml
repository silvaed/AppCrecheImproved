@page
@model S00190492_Creche.Pages.Student.ListStudentsModel
@{
    ViewData["Student"] = "List Students";
}


<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
        <th>Last Name</th>
    </tr>
    @foreach (var student in Model.Students)
    {
        <tr>
            <td>@student.ChildFirstName</td>
            <td>@student.ChildLastName</td>
        </tr>
    }
</table>    

